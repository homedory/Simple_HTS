<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Initial page</title>
</head>
<body>
    <h1>Home Trading System</h1>
    <button id="login">로그인</button>
    <button id="signup">회원가입</button>
    <button id="myinfo">내정보</button>
    <button id="stock">거래가능한 종목 보기</button>
    <script>
        const loginButton = document.getElementById("login");
        const signupButton = document.getElementById("signup");
        const myinfoButton = document.getElementById("myinfo");
        const allStocksButton = document.getElementById("stock");

        loginButton.addEventListener('click', () => {
            window.location = 'http://localhost:3000/login';
        });

        signupButton.addEventListener('click', () => {
            window.location = 'http://localhost:3000/signup';
        });

        myinfoButton.addEventListener('click', () => {
            window.location = 'http://localhost:3000/myinfo';
        });

        allStocksButton.addEventListener('click', () => {
            window.location = 'http://localhost:3000/stock';
        });
    </script>
</body>
</html>
